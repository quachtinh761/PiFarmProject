<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <stroke
                android:color="@color/loginButtonPressed"
                android:width="1dp"></stroke>
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"></padding>
            <solid
                android:color="@color/loginButtonPressed"></solid>
        </shape>
    </item>
    <item>
        <shape
            android:shape="rectangle">
            <stroke
                android:color="@color/black"
                android:width="1dp"></stroke>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"></padding>
            <solid
                android:color="@color/loginBackground"></solid>
        </shape>
    </item>
</selector>